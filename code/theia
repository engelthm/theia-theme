<!-- START SCRIPTS -->
    <!-- jquery import --> <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <!-- scroll to top --> <script src="https://static.tumblr.com/ikeq9mi/DfYl6o46t/scrolltotop.min.js"></script>
    <!-- scroll reveal --> <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
    <!-- tippy.js --> <script src="https://unpkg.com/popper.js@1"></script> <script src="https://unpkg.com/tippy.js@5/dist/tippy-bundle.iife.js"></script>
    <!-- audio fix by annasthms --> <script src="https://cdn.jsdelivr.net/gh/annasthms/customaudio@latest/customaudio.min.js"></script>
    <!-- no.js photosets by annasthms and eggdesign --> <script src="https://static.tumblr.com/0podkko/jfMplm3f8/modified_photosets.js"></script>
    <!-- unnest captions by neothm --> <script src="https://static.tumblr.com/wgg6svp/OoTofxa0c/unnest.min.js"></script>
    <!-- npf photosets fix by glenthemes --> <script src="//npf-images-v3.github.io/script.js"></script>

<!-- start js -->
<script>
    // aos scroll reveal
    AOS.init();
        
    // custom tooltips from tippy.js
    tippy('a[title]', {
        theme: 'custom',
        duration: 0,
        arrow: false,
        animation: 'shift-toward-subtle',
        followCursor: true,
        zIndex: 9999999999,
        content(reference) {
            const title = reference.getAttribute('title');
            reference.removeAttribute('title');
            return title;
        },
    });
    
    // controls toggle
    $(".contr").click(function(){
        $(".tmblr-iframe").toggle(200);
    });
    
    // audio fix by annasthms
    customAudio({
    	post: ".posts",
    	wrappers: {
    		audio: ".custom_audio_wrapper",
    		buttons: ".custom_audio_buttons",
    		seekbar: ".custom_audio_seekbar"
    	},
    	default: false,
    	pauseAll: true,
    	playButton: "<i data-feather='play'></i>",
    	pauseButton: "<i data-feather='pause'></i>",
    	errorIcon: "<i data-feather='x'></i>",
    	hideInfoIfError: true,
    	callAfterLoad: () => {
    		feather.replace();
    	}
    });
    
    // short note numbers by shythemes
    var $container = $('.info');
       $container.find('.notecount').each(function(){
           var n = $(this).html().split(' ')[0].replace(/,/g, '');
           if (n > 999) {
               n = Math.floor(n / 100) / 10;
               $(this).text(n + 'k');
           }
       });
       
    // unnest captions by neothm
    $(function(){  $('.posts').unnest({ 
        yourCaption: ".c",
        newCaptionUsername: true,
        originalPostCaptionUsername: true,
        usernameColon: true,
        tumblrAvatars: false
    }); }); 
    
    // no.js photosets by annasthms and eggdesign 
    initPhotosets();
    
</script>
<!-- END JS SCRIPTS -->


